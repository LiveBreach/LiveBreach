{
  "name": "live-breach",
  "version": "0.1.0",
  "private": true,
  "main": "dist/electron/main.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "electron:build": "tsc -p tsconfig.electron.json",
    "electron:start": "electron . --no-sandbox",
    "electron:dev": "npm run electron:build && cross-env ELECTRON_START_URL=http://localhost:3000 npm run electron:start",
    "dev:all": "concurrently -k \"npm run dev\" \"wait-on tcp:3000 && npm run electron:dev\""
  },
  "dependencies": {
    "next": "16.1.4",
    "puppeteer": "^24.35.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "ts-morph": "^27.0.2"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "electron": "^40.0.0",
    "electron-builder": "^26.4.0",
    "eslint": "^9",
    "eslint-config-next": "16.1.4",
    "tailwindcss": "^4",
    "typescript": "^5",
    "wait-on": "^9.0.3"
  }
}